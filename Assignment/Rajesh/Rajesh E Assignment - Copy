      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "d = {'k1':[1,2,3,{'tricky':['oh','man','inception',{'target':[1,2,3,'hello']}]}]}\n",
        "one = d['k1']\n",
        "two = one[3]\n",
        "three = two['tricky']\n",
        "four = three[3]\n",
        "five = four['target']\n",
        "six = five[3]\n",
        "print(six)"
      ],
      "metadata": {
